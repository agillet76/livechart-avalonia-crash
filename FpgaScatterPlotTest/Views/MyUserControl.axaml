<UserControl xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:FpgaScatterPlotTest.Views"
        x:Class="FpgaScatterPlotTest.Views.MyUserControl"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Name="root"
        >

    <StackPanel Orientation="Vertical">
        <Border BorderBrush="Black" BorderThickness="2" Width="400" Height="200" Margin="10">
            <TextBlock Text="{Binding MyProp, ElementName=root}" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        </Border>
        
    </StackPanel>
   
</UserControl>
